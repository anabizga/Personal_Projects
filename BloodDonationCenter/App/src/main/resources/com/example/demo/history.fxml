<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: BB1717;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.History">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: FFFFFF;">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="25.0" layoutY="225.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Label fx:id="history" layoutX="249.0" layoutY="76.0" text="Your donation history" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="30.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="281.0" layoutY="524.0" mnemonicParsing="false" style="-fx-background-color: FFFFFF; -fx-cursor: hand;" text="RETURN TO HOME PAGE" textFill="#6d1616">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font>
      </Button>
      <TableView fx:id="historyTableView" layoutX="245.0" layoutY="154.0" prefHeight="305.0" prefWidth="313.0">
         <columns>
            <TableColumn fx:id="quantityColumn" prefWidth="86.40003967285156" text="Quantity" />
            <TableColumn fx:id="dateColumn" prefWidth="89.20001220703125" text="Date" />
            <TableColumn fx:id="centerNameColumn" prefWidth="136.79998779296875" text="Center Name" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
