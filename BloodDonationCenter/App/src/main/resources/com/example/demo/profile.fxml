<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: BB1717;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Profile">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: FFFFFF;">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="30.0" layoutY="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <Label fx:id="details" layoutX="300.0" layoutY="84.0" text="Profile details" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="34.0" />
         </font>
      </Label>
      <Label fx:id="username" layoutX="241.0" layoutY="172.0" text="Username" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="firstname" layoutX="241.0" layoutY="279.0" text="First name" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="lastname" layoutX="241.0" layoutY="333.0" text="Last name" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="cnp" layoutX="241.0" layoutY="390.0" text="CNP" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="blood" layoutX="241.0" layoutY="442.0" text="Blood type" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="284.0" layoutY="511.0" mnemonicParsing="false" style="-fx-background-color: FFFFFF; -fx-cursor: hand;" text="RETURN TO HOME PAGE" textFill="#6f1212">
         <font>
            <Font name="Century Gothic Bold" size="20.0" />
         </font></Button>
      <Label fx:id="email" layoutX="241.0" layoutY="226.0" text="Email" textFill="WHITE">
         <font>
            <Font name="Century Gothic Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
