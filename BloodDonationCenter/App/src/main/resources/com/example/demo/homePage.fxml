<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HomePage">
   <children>
      <HBox prefHeight="600.0" prefWidth="600.0">
         <children>
            <AnchorPane prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: FFFFFF;">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="25.0" layoutY="225.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@logo.png" />
                     </image>
                  </ImageView>
               </children></AnchorPane>
            <AnchorPane prefHeight="571.0" prefWidth="400.0" style="-fx-background-color: BB1717;">
               <children>
                  <Label fx:id="welcome" layoutX="49.0" layoutY="74.0" text="Welcome username!" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic Bold" size="30.0" />
                     </font>
                  </Label>
                  <Button fx:id="donate_request" layoutX="127.0" layoutY="195.0" mnemonicParsing="false" style="-fx-background-color: FFFFFF; -fx-cursor: hand;" text="DONATE" textFill="#630f0f">
                     <font>
                        <Font name="Century Gothic Bold" size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="history" layoutX="127.0" layoutY="289.0" mnemonicParsing="false" style="-fx-background-color: FFFFFF; -fx-cursor: hand;" text="HISTORY" textFill="#630f0f">
                     <font>
                        <Font name="Century Gothic Bold" size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="add_cond" layoutX="32.0" layoutY="380.0" mnemonicParsing="false" style="-fx-background-color: FFFFFF; -fx-cursor: hand;" text="ADD MEDICAL CONDITION" textFill="#630f0f">
                     <font>
                        <Font name="Century Gothic Bold" size="24.0" />
                     </font>
                  </Button>
                  <Button fx:id="log_out" layoutX="140.0" layoutY="520.0" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-cursor: hand;" text="Log out" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="profile" layoutX="96.0" layoutY="475.0" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-cursor: hand;" text="View your profile" textFill="WHITE">
                     <font>
                        <Font name="Century Gothic" size="20.0" />
                     </font>
                  </Button>
               </children></AnchorPane>
         </children></HBox>
   </children>
</AnchorPane>
